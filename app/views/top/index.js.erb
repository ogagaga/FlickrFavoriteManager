$("#explor_content").hide();
$("#search_content").show();
$("#search_result").html("<%= escape_javascript(render :partial => 'top/search_result' ) %>");

var $search_result = $('#search_result');
$search_result.imagesLoaded(function(){
  $('#search_result').masonry({
    itemSelector : '.search_item',
    isAnimated: true,
    isFitWidth: true
  });
});

