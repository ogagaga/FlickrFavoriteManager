$("#explor_section").hide();
$("#search_section").show();
$("#search_content").html("<%= escape_javascript(render :partial => 'top/search_result' ) %>");

var $search_content = $('#search_content');
$search_content.imagesLoaded(function(){
  $('#search_content').masonry({
    itemSelector : '.search_item',
    isAnimated: true,
    isFitWidth: true
  });
});


